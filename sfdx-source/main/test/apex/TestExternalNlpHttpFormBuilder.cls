@isTest
private class TestExternalNlpHttpFormBuilder {
    @IsTest
    private static void testExternalNlpHttpFormBuilder() {
        blob fileABlob = EncodingUtil.base64Decode('');
        blob fileBBlob = EncodingUtil.base64Decode('');
        blob fileCBlob = EncodingUtil.base64Decode('');
        string formA64 = '';
        string formB64 = '';
        string formC64 = '';

        Test.startTest();

        string contentType = ExternalNlpHttpFormBuilder.GetContentType();

        formA64 += ExternalNlpHttpFormBuilder.WriteBoundary();
        formA64 += ExternalNlpHttpFormBuilder.WriteBodyParameter('key', 'value');
        formA64 += ExternalNlpHttpFormBuilder.WriteBoundary();

        ExternalNlpHttpFormBuilder.WriteFileResult resultA = ExternalNlpHttpFormBuilder.WriteFile('key', 'value', 'image/jpeg', fileABlob);

        formA64 += resultA.Content;
        formA64 += ExternalNlpHttpFormBuilder.WriteBoundary(resultA.EndingType);

        formB64 += ExternalNlpHttpFormBuilder.WriteBoundary();
        formB64 += ExternalNlpHttpFormBuilder.WriteBodyParameter('key', 'value');
        formB64 += ExternalNlpHttpFormBuilder.WriteBoundary();

        ExternalNlpHttpFormBuilder.WriteFileResult resultB = ExternalNlpHttpFormBuilder.WriteFile('key', 'value', 'image/jpeg', fileBBlob);

        formB64 += resultB.Content;
        formB64 += ExternalNlpHttpFormBuilder.WriteBoundary(resultB.EndingType);

        formC64 += ExternalNlpHttpFormBuilder.WriteBoundary();
        formC64 += ExternalNlpHttpFormBuilder.WriteBodyParameter('key', 'value');
        formC64 += ExternalNlpHttpFormBuilder.WriteBoundary();

        ExternalNlpHttpFormBuilder.WriteFileResult resultC = ExternalNlpHttpFormBuilder.WriteFile('key', 'value', 'image/jpeg', fileCBlob);

        formC64 += resultC.Content;
        formC64 += ExternalNlpHttpFormBuilder.WriteBoundary(resultC.EndingType);

        Test.stopTest();

        System.assert(contentType == 'multipart/form-data; charset="UTF-8"; boundary="1ff13444ed8140c7a32fc4e6451aa76d"');
        System.assert(formA64 == '');
        System.assert(formB64 == '');
        System.assert(formC64 == '');
    }
}
